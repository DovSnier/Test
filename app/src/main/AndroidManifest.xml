<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dvsnier.test">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name="com.dvsnier.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TestCrashHandleActivity"
            android:theme="@style/AppCustomTheme" />
        <activity
            android:name="com.dvsnier.testAffinity.AffinityActivity"
            android:launchMode="singleTask" />
        <activity
            android:name="com.dvsnier.testAffinity.AActivity"
            android:taskAffinity="com.dvsnier.testAffinity" />
        <activity
            android:name="com.dvsnier.testAffinity.BActivity"
            android:taskAffinity="com.dvsnier.testAffinity" />
        <activity android:name="com.dvsnier.testAffinity.CActivity" />
    </application>

</manifest>